{
  "name": "palindrome",
  "version": "0.0.0-development",
  "private": true,
  "description": "React Code Project",
  "keywords": [],
  "bugs": {
    "url": "https://github.com/kylesalter/palindrome"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kylesalter/palindrome"
  },
  "author": "Kyle Salter <me@kylesalter.com>",
  "scripts": {
    "prebuild": "yarn typecheck",
    "build": "gatsby build",
    "build-storybook": "build-storybook",
    "predeploy": "yarn build",
    "deploy": "netlify deploy",
    "develop": "gatsby develop",
    "docs": "styleguidist server",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx --format=pretty",
    "format": "run-p format:*",
    "format:doctoc": "doctoc --maxlevel 3 --notitle ./README.md",
    "format:imports": "import-sort --write '*.{js,jsx,ts,tsx}'",
    "format:package-json": "sort-package-json ./package.json ",
    "format:prettier": "prettier --write '**/*.{css,gql,graphql,html,js,jsx,json,less,md,mdx,scss,ts,tsx,vue,yaml,yml}' '.editorconfig' 'LICENSE'",
    "format:src": "yarn lint:fix",
    "lint": "run-p lint:js lint:styles",
    "lint:fix": "run-s lint:*:fix",
    "lint:js": "yarn eslint ./",
    "lint:js:fix": "yarn eslint --fix ./",
    "lint:staged": "lint-staged",
    "lint:styles": "yarn stylelint './src/**/*.{js,jsx,ts,tsx,css,scss}'",
    "lint:styles:fix": "yarn stylelint --fix './src/**/*.{js,jsx,ts,tsx,css,scss}'",
    "lint:watch": "onchange 'src/**/*' --initial --kill --delay 1000  -- yarn lint",
    "start": "yarn develop",
    "test": "jest",
    "typecheck": "yarn tsc --project ./tsconfig.json --noEmit --pretty"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint:staged build test"
    }
  },
  "browserslist": [">0.2%", "not dead", "not ie <= 11", "not op_mini all"],
  "dependencies": {
    "@emotion/core": "^10.0.15",
    "@emotion/styled": "^10.0.15",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "@types/react-helmet": "^5.0.9",
    "@types/webfontloader": "^1.6.29",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "bootstrap": "^4.3.1",
    "gatsby": "^2.13.64",
    "gatsby-image": "^2.2.8",
    "gatsby-plugin-catch-links": "^2.1.3",
    "gatsby-plugin-emotion": "^4.1.2",
    "gatsby-plugin-manifest": "^2.2.5",
    "gatsby-plugin-offline": "^2.2.6",
    "gatsby-plugin-react-helmet": "^3.1.3",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sitemap": "^2.2.6",
    "gatsby-plugin-typescript": "^2.1.2",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-transformer-remark": "^2.6.14",
    "jquery": "^3.4.1",
    "manacher": "^1.1.1",
    "moment": "2.29.4",
    "palindrome-plus": "^1.0.2",
    "popper.js": "^1.15.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-popper": "^1.3.4",
    "react-transition-group": "^4.2.2",
    "reactstrap": "^8.0.1",
    "sanitize.css": "^11.0.0",
    "shortid": "^2.2.14"
  },
  "devDependencies": {
